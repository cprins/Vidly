@model Vidly.Models.Customer

@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>

    <li><b>Tipo de membresía: &nbsp </b> @Model.MembershipType.Name</li>
        @if(Model.Birthdate.HasValue)
        {
    <li><b>Cumpleaños: &nbsp </b>@Model.Birthdate.Value.ToShortDateString()</li>
        }
</ul>



